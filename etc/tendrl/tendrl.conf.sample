[common]

# Central store etcd port
etcd_port = 2379

# Central store etcd host/ip
etcd_connection = 0.0.0.0

# Path to log file
log_path = /var/log/tendrl/common.log
log_level = DEBUG


[alerting]
# Path to log file
log_cfg_path = /etc/tendrl/alerting_logging.yaml
log_level = DEBUG
api_server_addr = 0.0.0.0
api_server_port = 5001